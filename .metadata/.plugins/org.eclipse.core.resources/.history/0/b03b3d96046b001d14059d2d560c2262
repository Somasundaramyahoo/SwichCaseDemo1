package examples;

public class StringIndexOutOfBoundsDemo {
	public static void main(String[] args) {
		String[] a={"somu","sundaram"};
		try{
			//System.out.println(a[2]);//Array indexOutOfBounds//
			System.out.println(a[4].length());
		}
		catch(Exception e){
			//System.out.println("StringIndexOutOfBoundsException error");
			e.printStackTrace(); 
		}finally{
		System.out.println("end");
		}
	}

}
